{
  "name": "trading-ai",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/server/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only server/index.ts",
    "build:server": "tsc -p tsconfig.server.json",
    "fix-imports-dist": "node ./fix-imports-dist.js",
    "build:frontend": "vite build --outDir dist/client",
    "build": "npm run build:server && npm run fix-imports-dist && npm run build:frontend",
    "start": "node ./dist/server/index.js",
    "start:web": "node ./dist/server/index.js",
    "frontend": "vite",
    "frontend:preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "lightweight-charts": "^5.0.8",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "rollup": "^3.29.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  },
  "engines": {
    "node": ">=18"
  }
}